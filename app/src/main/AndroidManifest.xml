<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.rejointech.planeta">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <application
        android:allowBackup="true"
        android:icon="@drawable/logoreal"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logoreal"
        android:supportsRtl="true"
        android:theme="@style/Theme.Planeta">
        <activity
            android:name=".Container.HomeActivityContainer"
            android:exported="true" >

        </activity>
        <activity
            android:name=".Startup.Startup_container"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.Planeta.NoActionBar">
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>